    "# Fibonacci number\n",
    "\n",
    "fibo =  [0,1]\n",
    "n = int(input(\"How many fibonacci numbers do you want:  \"))\n",
    "\n",
    "for i in range(n):\n",
    "    if max(fibo) == 55:\n",
    "        break\n",
    "    x = fibo[i] + fibo[i+1]\n",
    "    fibo.append(x)\n",
    "print(fibo)    "
   ]
