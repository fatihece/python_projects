    "n = int(input())\n",
    "prime_num = []\n",
    "\n",
    "for j in range(n + 1):\n",
    "    control = False\n",
    "    if j > 1:\n",
    "        for i in range(2, j):\n",
    "            if (j % i) == 0:\n",
    "                control = True\n",
    "        if not control:\n",
    "            prime_num.append(j)\n",
    "        else:\n",
    "            pass\n",
    "print(prime_num) "
   ]
